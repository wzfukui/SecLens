{
  "slug": "atlassian_security",
  "name": "Atlassian Security Advisories Collector",
  "version": "1.0.4",
  "entrypoint": "collector:run",
  "description": "Fetches security advisories from Atlassian vulnerability transparency API",
  "schedule": "14400",
  "source": {
    "publisher": "Atlassian",
    "homepage": "https://www.atlassian.com/trust/data-protection/vulnerabilities",
    "feed_url": "https://www.atlassian.com/gateway/api/vuln-transparency/v1/products"
  },
  "runtime": {
    "ingest_url": "https://host/v1/ingest/bulletins",
    "token": "<api token>"
  },
  "time_policy": {
    "default_timezone": "UTC",
    "naive_strategy": "assume_default",
    "max_future_drift_minutes": 120,
    "max_past_drift_days": 30
  },
  "ui": {
    "group_slug": "vendor_updates",
    "group_title": "厂商更新",
    "group_description": "各大厂商发布的安全公告、漏洞信息和产品更新。",
    "group_order": 20,
    "source_title": "Atlassian 安全公告",
    "source_order": 10
  }
}